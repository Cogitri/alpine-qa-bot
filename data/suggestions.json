{
    "commit": [
        {
            "offenders": [
                ".*\/.*: update to.*"
            ],
            "suggestion": "$repository/$pkgname: upgrade to $pkgver"
        },
		{
			"offenders": [
				".*\/.*: move to.*"
			],
			"suggestion": "Please use 'move from' instead of 'move to'"
		},
		{
			"offenders": [
				".*\/.*: import from.*"
			],
			"suggestion": "Please use 'move from' instead of 'import from'"
		}
    ]
}
