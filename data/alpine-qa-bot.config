[Server]
# URL to the Gitlab instance
GitlabUrl=https://gitlab.com
# Token Gitlab sends to authenticate itself
GitlabToken=
# Port to listen on for notification from Gitlab webhooks
Port=80
# Access token used for authenticating against the gitlab API.
AuthenticationToken=

[Poller]
# Project IDs that should be polled for new events (e.g. failing pipelines)
ProjectIds=0;1
# How many seconds between poller runs. Make sure this isn't too low,
# or label adding/removing might be spammy
Period=3600